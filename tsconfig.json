{
  "files": [],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "app/*"
      ],
      "@content/*": [
        "content/*"
      ],
      "@server/*": [
        "server/*"
      ],
      "@shared/*": [
        "shared/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@prisma/*": [
        "prisma/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@types/*": [
        "types/*"
      ],
      "#prisma": [
        "prisma/prisma.ts"
      ]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "@types/node"
    ]
  },
  "references": [
    {
      "path": "./.nuxt/tsconfig.app.json"
    },
    {
      "path": "./.nuxt/tsconfig.server.json"
    },
    {
      "path": "./.nuxt/tsconfig.shared.json"
    }
  ]
}