{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "types": [
      "@nuxt/content"
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "#content": [
        "./.nuxt/content/index.d.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    ".nuxt"
  ]
}