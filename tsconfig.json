{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "types": [
      "@types/node"
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@assets/*": [
        "./assets/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}