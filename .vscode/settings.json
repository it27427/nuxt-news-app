{
  "workbench.sideBar.location": "right",
  "workbench.colorTheme": "JellyFish",
  "workbench.iconTheme": "helium-icon-theme",
  "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font",
  "terminal.integrated.fontSize": 17,
  "terminal.integrated.fontWeight": "600",
  "terminal.integrated.cursorBlinking": true,
  "editor.fontFamily": "Consolas, 'Courier New', monospace",
  "editor.fontSize": 18,
  "files.autoSave": "onFocusChange",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "nuxt-intellisense.showWelcomeMessage": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "html.format.contentUnformatted": "pre,code,textarea",
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.fixAll.eslint": "always",
    "source.fixAll.stylelint": "always",
    "source.fixAll.tslint": "always",
    "source.organizeImports": "always"
  },
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "editor.formatOnType": false,
  "prettier.singleQuote": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "typescript",
    "typescriptreact"
  ],
  "html.format.enable": false,
  "html.autoClosingTags": false,
  "editor.codeActions.triggerOnFocusChange": true,
  "editor.formatOnPaste": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "vue.inlayHints.destructuredProps": true,
  "vue.inlayHints.inlineHandlerLeading": true,
  "vue.inlayHints.missingProps": true,
  "vue.inlayHints.vBindShorthand": true,
  "vue.suggest.componentNameCasing": "alwaysPascalCase",
  "vue.suggest.propNameCasing": "alwaysKebabCase",
  "vue.trace.server": "messages",
  "vue.autoInsert.dotValue": true,
  "RainbowBrackets.forceIterationColorCycle": true,
  "RainbowBrackets.highlightActiveScope": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "vue-tag-highlight.color.script": "rgba(255, 255, 255, 0.2)",
  "vue-tag-highlight.color.template": "rgba(0, 0, 255, 0.2)",
  "vue-tag-highlight.color.style": "rgba(255, 0, 0, 0.2)",
  "vue-tag-highlight.isShowOnTextEditor": true,
  "vue-tag-highlight.isShowOnScrollbar": true,
  "highlight-matching-tag.enabled": true,
  "highlight-matching-tag.showPath": true,
  "highlight-matching-tag.showRuler": true,
  "highlight-matching-tag.highlightSelfClosing": true,
  "highlight-matching-tag.highlightFromContent": true,
  "highlight-matching-tag.highlightFromName": true,
  "highlight-matching-tag.highlightFromAttributes": true,
  "highlight-matching-tag.noDefaultEmptyElements": true,
  "highlight-matching-tag.styles": {
    "opening": {
      "name": {
        "underline": "yellow"
      }
    }
  },
  "css.lint.unknownAtRules": "ignore",
  "css.lint.unknownProperties": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "scss.lint.unknownProperties": "ignore",
  "terminal.integrated.tabs.defaultIcon": "vm",
  "tailwindCSS.lint.invalidVariant": "ignore",
  "tailwindCSS.lint.invalidTailwindDirective": "ignore",
  "tailwindCSS.lint.invalidScreen": "ignore",
  "tailwindCSS.lint.invalidConfigPath": "ignore",
  "iis.configDir": "",
}